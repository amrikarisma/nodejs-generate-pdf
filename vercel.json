{
    "version": 2,
    "name": "nodejs-generate-pdf",
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        },
        {
            "src": "./files/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/files/(.*)"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}